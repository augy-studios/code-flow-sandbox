{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "cleanUrls": true,
    "regions": ["sin1"],
    "rewrites": [{
        "source": "/",
        "destination": "/"
    }],
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "X-Content-Type-Options",
                "value": "nosniff"
            },
            {
                "key": "Referrer-Policy",
                "value": "strict-origin-when-cross-origin"
            }
        ]
    }]
}